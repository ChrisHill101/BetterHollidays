doctype
html(lang="en")
    head
        title View All Destinations - Better Holidays
        link(href='styles.css' rel='stylesheet')
        link(href='Fonts/font-awesome-4.7.0/css/font-awesome.min.css' rel='stylesheet')
        link(href="https://fonts.googleapis.com/css?family=Baloo+Bhaina" rel="stylesheet")
        link(href="https://fonts.googleapis.com/css?family=Lobster" rel="stylesheet")
        link(href="https://fonts.googleapis.com/css?family=Bree+Serif" rel="stylesheet")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")

    body
        div#MainBody
            div#nav.fixed
                    ul.links
                        li
                            a.menu(href="home.html") Home
                        li
                            a(href="#")#DestDropDown.active Destinations <i class="fa fa-caret-down" aria-hidden="true"></i>
                            #Dest.DropDown    
                                    ul
                                        li
                                            a(href="#") <i class="fa fa-caret-right" aria-hidden="true"></i> Summer Sun
                                        li
                                            a(href="#") <i class="fa fa-caret-right" aria-hidden="true"></i> Winter Sun
                                        li
                                            a(href="#") <i class="fa fa-caret-right" aria-hidden="true"></i> City Breaks
                                        li
                                            a.Selected(href="#") <i class="fa fa-caret-right" aria-hidden="true"></i> View All
                        li
                            a.menu(href="1.html") Accomodation
                        li
                            a.menu(href="#") Deals
                        li
                            a.menu(href="#") Contact Us
                    ul.btns
                        li
                            a.menu(href="https://twitter.com/" target="_blank") <i class="fa fa-twitter" aria-hidden="true"></i>
                        li
                            a.menu(href="#") <i class="fa fa-envelope" aria-hidden="true"></i>
            div.SliderContainer
                div.Slider
                    img(src="Images/DestinationsSlider/1.jpg")
                    img(src="Images/DestinationsSlider/2.jpg")
                    img(src="Images/DestinationsSlider/3.jpg")
                    img(src="Images/DestinationsSlider/4.jpg")
                    img(src="Images/DestinationsSlider/1.jpg")
            Div.MsgPopup.Hidden
                .Help
                    i#CloseHelpPopup(class="fa fa-times-circle" aria-hidden="true")
                    img(src="Images/7043dd1b121f1a9fa077caa6961fccc1.jpeg")
                    h3 Luke Travis
                    h5 Offline
                p Hi! If you need any help don't hesitate to message me and I will get back to you as soon as I can!
                h4.message Message:
                textarea(rows="6" placeholder="Message")
            div.HelpBtn.Show
                  h3 Help
                  i(class="fa fa-question-circle" aria-hidden="true")
            
